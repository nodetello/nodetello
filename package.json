{
  "name": "nodetello",
  "version": "0.0.1",
  "description": "A Teenage Mutant Node Boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint .",
    "dev": "NODE_ENV=development eslint . && cd ./src && mocha && cd .. && nodemon src/index.js",
    "prd": "NODE_ENV=production eslint . && pm2 start ecosystem.config.js",
    "test": "NODE_ENV=test eslint . && cd ./src && mocha",
    "debug": "node --nolazy --inspect-brk=3001 src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stanleygomes/nodetello"
  },
