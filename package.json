  "bugs": {
    "url": "https://github.com/stanleygomes/nodetello"
  },
  "homepage": "https://github.com/stanleygomes/nodetello",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.21.2",
    "i18n": "^0.8.3",
    "moment": "^2.24.0",
    "mustache": "^3.0.1",
    "pm2": "^3.5.0",
    "queue": "^6.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.17.3",
